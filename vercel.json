{
    "version": 2,
    "name": "app-payment-mp-envioflores",
    "builds": [
       { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/app.js" }
    ], 
    
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api" }
    ]
      
    
 }